<h1>documents#edit</h1>

<%= form_with model: @document, local:true do |f| %>
　<label for="inputImage">がぞう</label>
  <%= f.attachment_field :diary_image, placeholder: "がぞう" %>
  <label for="inputTitle">タイトル</label>
  <%= f.text_field :title, placeholder: "タイトル" %>
  <label for="inputBody">ぼうけんのないよう</label>
  <%= f.text_field :body, placeholder: "ぼうけんのないよう" %>
  <%= f.submit 'きろくしなおす' %>
<% end %>